<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN" "http://ibatis.apache.org/dtd/sql-map-2.dtd" >
<sqlMap namespace="Mcodes">

	<typeAlias  alias="egovMap" type="egovframework.rte.psl.dataaccess.util.EgovMap"/>
	<typeAlias  alias="McodeVO" type="egovframework.goncs.member.service.McodeVO"/>
	<typeAlias  alias="McodeVOs" type="egovframework.goncs.member.service.McodeVO"/>
		
	<select id="mcodeDAO.selectMcodeList" resultClass="McodeVOs">
		<![CDATA[ 
			SELECT *
			FROM tb_code
			WHERE 1=1 
			AND maxcod = #maxcod#
			ORDER BY code
		]]>
	</select>
	
</sqlMap>	