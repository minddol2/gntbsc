<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN" "http://ibatis.apache.org/dtd/sql-map-2.dtd" >

<sqlMap namespace="LoginUsr">

	<typeAlias  alias="egovMap" type="egovframework.rte.psl.dataaccess.util.EgovMap"/>
	<typeAlias  alias="loginVO" type="egovframework.goncs.member.service.LoginVO"/>
	<typeAlias  alias="loginVOs" type="egovframework.goncs.member.service.LoginVO"/>
	
	<!-- 일반 로그인 AND a.ci_LoginPW = password(#ci_LoginID#)  -->
	<select id="loginDAO.actionLogin" parameterClass="loginVO" resultClass="loginVOs">
		<!-- 일반회원 -->
		<![CDATA[
		SELECT *
		  FROM member
		 WHERE us_id = #us_id#
		    AND us_pw = #us_pw#
		]]>
		ORDER BY idx desc Limit 1
	</select>
	
			
</sqlMap>
